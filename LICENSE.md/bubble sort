int main()
{
   int a[]={23,45,98,78,4,12};
   int i;
   bubble(a,6);
   for(i=0;i<=6;i++)
     printf("%d ",a[i]);
    getch();
  }
  void bubble(int a[],int n)
  {
    int b,i,temp;
    for(b=0;b<=n-1;b++)
      for(i=0;i<=n-1-b;i++)
        if(a[i]>a[i+1])
        {
         temp=a[i];
         a[i]=a[i+1];
         a[i+1]=temp;
         }
  }
